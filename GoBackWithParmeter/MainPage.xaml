<Page
    x:Class="GoBackWithParmeter.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:GoBackWithParmeter"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <StackPanel VerticalAlignment="Center">
            <Button Content="Scan" Click="Button_Click"  HorizontalAlignment="Center"/>
            <TextBlock Text="Message" TextAlignment="Center" Margin="0,15,0,0"  FontStyle="Normal" FontSize="30" />
            <TextBlock Name="Info" Margin="0,50,0,0" Text="dddd" FontSize="55"/>
        </StackPanel>
    </Grid>
</Page>
